{
  "expo": {
    "name": "expo1",
    "slug": "expo1",
    "scheme": "myapp",
    
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [{
            "scheme": "myapp"
          }],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],

    }
 
  }
}
